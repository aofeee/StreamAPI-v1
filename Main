import java.util.*;

public class Main {

    public static void main(String[] args) {
        System.out.println("Вариант без стрима");
        List<Integer> intList = Arrays.asList(1, 2, 5, 16, -1, -2, 0, 32, 3, 5, 8, 23, 4);
        for (int i :
                intList) {
            intList.sort(Comparator.naturalOrder());
            if (i > 0 && i % 2 == 0) {
                System.out.println(i);
            }
        }
    }
}
